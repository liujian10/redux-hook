{
  "name": "@maple-rc/redux-hook",
  "description": "Use hooks realize redux in react",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liujian10/redux-hook.git"
  },
  "keywords": [
    "react",
    "hook",
    "hooks",
    "redux"
  ],
  "author": "mapleliu1@gmail.com",
  "main": "lib/hooks/index.js",
  "module": "esm/hooks/index.js",
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/hooks/index.d.ts",
  "typings": "lib/hooks/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint {src,tests,stories}/**/*.{ts,tsx}",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn clean && yarn build && yarn test"
    }
  },
  "lint-staged": {
    "src/**/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.16.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "7.21.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "bugs": {
    "url": "https://github.com/liujian10/redux-hook/issues"
  },
  "homepage": "https://github.com/liujian10/redux-hook#readme",
  "directories": {
    "lib": "lib"
  },
  "license": "ISC"
}
